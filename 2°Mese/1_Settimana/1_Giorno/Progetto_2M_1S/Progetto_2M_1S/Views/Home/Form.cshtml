@model Dao_2M_1S.Model.Clienti

<form asp-action="BottoneForm" method="post">
    <div class="form-group">
        <label asp-for="Nome" class="control-label"></label>
        <input asp-for="Nome" class="form-control" />
        <span asp-validation-for="Nome" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Cognome" class="control-label"></label>
        <textarea asp-for="Cognome" class="form-control"></textarea>
        <span asp-validation-for="Cognome" class="text-danger"></span>
    </div>
    <div class="form-group">
        <div class="form-check">
            <input asp-for="IsPrivate" class="form-check-input" type="checkbox" />
            <label asp-for="IsPrivate" class="form-check-label"></label>
        </div>
        <span asp-validation-for="IsPrivate" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Eta" class="control-label"></label>
        <input asp-for="Eta" class="form-control" />
        <span asp-validation-for="Eta" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Cf" class="control-label"></label>
        <textarea asp-for="Cf" class="form-control"></textarea>
        <span asp-validation-for="Cf" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="P_Iva" class="control-label"></label>
        <textarea asp-for="P_Iva" class="form-control"></textarea>
        <span asp-validation-for="P_Iva" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-outline-dark my-4">Aggiungi</button>
</form>