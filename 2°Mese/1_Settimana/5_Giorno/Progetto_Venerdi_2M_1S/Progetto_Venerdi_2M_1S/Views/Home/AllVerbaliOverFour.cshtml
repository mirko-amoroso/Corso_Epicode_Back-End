
@model List<Prog_S1_V.Model.Anagrafica_Verbale>

    <h2>Tutti le multe il cui importo è superiore a 200&#8364; :</h2>

    @foreach (var verbale in Model)
    {
        <div class="card">
          <div class="card-body">
            <h5 class="card-title">L'utente:@verbale.Nome  @verbale.Cognome</h5>
            <h6 class="card-subtitle mb-2 text-body-secondary">In Data: @verbale.DataViolazione</h6>
            <h6 class="card-subtitle mb-2 text-body-secondary">Costo Multa: @verbale.Importo</h6>
            <p class="card-text">Totali punti decurtati: @verbale.DecurtamentoPunti</p
          </div>
        </div>
    }

