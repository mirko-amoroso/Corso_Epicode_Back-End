@model List<Prog_S1_V.Model.Anagrafica_Verbale>

@foreach (var verbale in Model)
{
    <div class="card">
      <div class="card-body">
        <h5 class="card-title">@verbale.NumeroVerbali</h5>
        <h6 class="card-subtitle mb-2 text-body-secondary">@verbale.Nome  @verbale.Cognome</h6>
        <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p
      </div>
    </div>
}
